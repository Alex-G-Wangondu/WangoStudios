{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}

